<%= @user.errors.inspect if @user.errors.present?%>

<%= form_for @user, :url =>  user_registration_path, :method => :post do |f| %>
<%= @a.inspect %>

	<%= f.text_field :email %>
	<%= f.hidden_field :name, value: @name %>
	<%= f.hidden_field :ip_address, value: @ip_address %>
	<%= f.fields_for :twitter_access_token do |builder| %>

		<%= builder.hidden_field :access_token, value: @access_token %>
		<%= builder.hidden_field :secret, value: @secret %>
		<%= builder.hidden_field :uid, value: @uid %>

	<% end %>
	
	<%= f.submit %>

<% end %>